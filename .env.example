# RinaPro WhatsApp ERP - Environment Variables

# Database (Optional - will use SQLite fallback if not set)
DATABASE_URL=mysql://user:password@localhost:3306/rinapro

# OAuth Server (Optional - for external authentication)
OAUTH_SERVER_URL=https://your-oauth-server.com

# WhatsApp Configuration
MANAGER_PHONE=966500000000

# AI Configuration (OpenAI for voice and text processing)
OPENAI_API_KEY=your-openai-api-key-here

# Application Settings
NODE_ENV=production
PORT=10000
